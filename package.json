{
	"name": "express-cms",
	"private": true,
	"workspaces": ["apps/*", "packages/*"],
	"scripts": {
		"start:all": "lerna run start",
		"prod:all": "lerna run prod",
		"test:all": "lerna run test",
		"clean:all": "npx lerna clean -y && npx lerna bootstrap",
		"start:api-server": "lerna run start --scope=api-server",
		"start:backend-cms": "lerna run start --scope=backend-cms",
		"sb": "lerna run start --scope=backend-cms",
		"prod:api-server": "lerna run prod --scope=consumer-api",
		"prod:backend-cms": "lerna run prod --scope=backend-cms",
		"watch": "npm run development -- --watch",
		"dev": "npm run development",
		"development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --config=node_modules/laravel-mix/setup/webpack.config.js",
		"hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
		"prod": "npm run production",
		"production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --config=node_modules/laravel-mix/setup/webpack.config.js",
		"lint": "echo 'Checking files for possible flaws in coding standards.....' && eslint \"**/*.js\" && lint-staged",
		"lint-fix": "echo 'Fixing files for possible flaws in coding standards.....' && eslint \"**/*.js\" --fix",
		"format": "prettier --write \"packages/**/*.js\"",
		"prettier:check": "prettier --check .",
		"prettier:write": "prettier --write .",
		"migrate": "npx sequelize db:migrate",
		"make:migration": "npx sequelize migration:generate --name",
		"migrate:undo": "npx sequelize db:migrate:undo",
		"migrate:undo:all": "npx sequelize db:migrate:undo:all",
		"make:seed": "npx sequelize seed:generate --name",
		"seed:all": "npx sequelize db:seed:all",
		"seed:one": "npx sequelize db:seed --seed",
		"seed:undo": "npx sequelize db:seed:undo",
		"seed:undo:all": "npx sequelize db:seed:all",
		"prepare": "husky install",
		"generate:module": "node packages/shared/src/cli/generate-module",
		"generate:db:model": "node packages/shared/src/cli/generate-db-model"
	},
	"dependencies": {
		"@hapi/boom": "^9.1.4",
		"awilix": "^7.0.3",
		"axios": "^0.21.1",
		"bcryptjs": "^2.4.3",
		"body-parser": "^1.20.1",
		"colors": "^1.4.0",
		"compression": "^1.7.4",
		"config": "^3.3.9",
		"connect-flash": "latest",
		"connect-redis": "^6.0.0",
		"connect-session-sequelize": "^7.1.1",
		"cookie-parser": "^1.4.6",
		"cors": "^2.8.5",
		"dotenv": "^16.0.3",
		"ejs": "3.1.8",
		"ejs-lint": "^1.2.1",
		"events": "^3.2.0",
		"express": "^4.18.2",
		"express-fileupload": "^1.1.4",
		"express-session": "latest",
		"express-validator": "^6.3.1",
		"http-status-codes": "^2.2.0",
		"method-override": "^3.0.0",
		"module-alias": "^2.2.2",
		"moment": "^2.24.0",
		"moment-precise-range": "^0.2.0",
		"moment-precise-range-plugin": "^1.3.0",
		"moment-timezone": "^0.5.28",
		"nodemailer": "^6.9.1",
		"oauth2orize": "^1.11.1",
		"passport": "latest",
		"passport-http": "^0.3.0",
		"passport-http-bearer": "^1.0.1",
		"passport-local": "^1.0.0",
		"passport-oauth2-client-password": "^0.1.2",
		"pg": "^8.9.0",
		"pg-hstore": "^2.3.4",
		"pluralize": "^8.0.0",
		"rand-token": "^0.4.0",
		"rate-limiter-flexible": "^2.2.4",
		"redis": "^3.1.2",
		"response-time": "^2.3.2",
		"sequelize": "^6.28.0",
		"sequelize-paginate": "^1.1.6",
		"socket.io": "^4.7.5",
		"socket.io-client": "^4.7.5",
		"vue": "^3.4.27",
		"vue-loader": "^17.4.2",
		"vue-template-compiler": "^2.7.16"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.1.2",
		"@commitlint/config-conventional": "^17.1.0",
		"cross-env": "^7.0.3",
		"eslint": "^8.33.0",
		"eslint-config-node": "^4.1.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-prettier": "^4.2.1",
		"husky": "^8.0.1",
		"laravel-mix": "^6.0.49",
		"lerna": "^6.4.1",
		"lint-staged": "^12.3.7",
		"nodemon": "^2.0.20",
		"prettier": "^2.6.0",
		"rimraf": "^3.0.2",
		"sass": "^1.58.0",
		"sass-loader": "^13.2.0",
		"sequelize-cli": "^6.6.0",
		"webpack-cli": "^5.0.1"
	}
}
